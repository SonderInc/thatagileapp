rules_version = '2';
// Firebase Storage rules. Copy to Firebase Console → Storage → Rules and publish.
// Upload path: tenants/{tenantId}/... (e.g. company logos). Authenticated users only.

service firebase.storage {
  match /b/{bucket}/o {
    match /tenants/{tenantId}/{allPaths=**} {
      allow read, write: if request.auth != null;
    }
  }
}
